# GuardianShield Environment Variables
# Copy this file to .env and fill in your actual values

# Supabase Configuration
VITE_SUPABASE_URL=https://your-project.supabase.co
VITE_SUPABASE_ANON_KEY=your-supabase-anon-key

# Stripe Configuration (for premium features)
VITE_STRIPE_PUBLISHABLE_KEY=pk_test_your-stripe-publishable-key
VITE_STRIPE_MONTHLY_PRICE_ID=price_monthly_subscription_id
VITE_STRIPE_YEARLY_PRICE_ID=price_yearly_subscription_id

# OpenAI Configuration (for AI-powered features)
VITE_OPENAI_API_KEY=your-openai-api-key

# Geocoding Service (for state detection)
VITE_GEOCODING_API_KEY=your-geocoding-api-key

# SMS Gateway (for emergency alerts)
VITE_SMS_GATEWAY_URL=https://your-sms-gateway.com/api

# App Configuration
VITE_APP_NAME=GuardianShield
VITE_APP_VERSION=1.0.0
VITE_APP_ENVIRONMENT=development

# Feature Flags
VITE_ENABLE_PREMIUM_FEATURES=true
VITE_ENABLE_VIDEO_RECORDING=true
VITE_ENABLE_CLOUD_BACKUP=true
VITE_ENABLE_AI_FEATURES=false

# Analytics (optional)
VITE_GOOGLE_ANALYTICS_ID=GA-XXXXXXXXX
VITE_MIXPANEL_TOKEN=your-mixpanel-token

# Error Reporting (optional)
VITE_SENTRY_DSN=https://your-sentry-dsn@sentry.io/project-id
